<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="orbital.gns.tpspacefinder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.FavouritesActivity"></activity>
        <activity android:name=".Activities.SearchBarActivity" />
        <activity android:name=".Activities.CrowdActivity" />
        <activity android:name=".Activities.ActionPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SeatSelectionActivity" />
        <activity android:name=".Activities.QRCodeActivity" />
        <activity android:name=".Activities.LogoutActivity" />
        <activity android:name=".Activities.SignUpActivity" />
        <activity android:name=".Activities.UpdateProfileActivity" />
        <activity android:name=".Activities.MapActivity" />
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.StartActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>